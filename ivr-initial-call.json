    {
      "parameters": {
        "jsCode": "const twiml = `<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n<Response>\n    <Say voice=\"Polly.Joanna\" language=\"en-US\">\n        Hello! Welcome to our knowledge system. Please ask your question clearly and briefly.\n    </Say>\n    <Gather \n        input=\"speech\" \n        action=\"https://berkaycann.app.n8n.cloud/webhook/voice-speech\" \n        method=\"POST\"\n        language=\"en-US\"\n        speechTimeout=\"auto\"\n        timeout=\"5\">\n    </Gather>\n    <Say voice=\"Polly.Joanna\" language=\"en-US\">\n        Sorry, I didn't catch that.\n    </Say>\n    <Hangup/>\n</Response>`;\n\nreturn [{\n  json: {\n    twiml: twiml\n  }\n}];"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        -304,
        -448
      ],
      "id": "dc3b8c94-28b0-4654-8069-1a2397553b50",
      "name": "TwiML Welcome Response"
    },
    {
      "parameters": {
        "jsCode": "// Twilio'dan gelen arama verileri\nconst from = $input.item.json.body.From || 'unknown';\nconst to = $input.item.json.body.To || 'unknown';\nconst callSid = $input.item.json.body.CallSid || 'unknown';\n\nreturn {\n  json: {\n    from: from,\n    to: to,\n    callSid: callSid,\n    platform: 'voice',\n    timestamp: new Date().toISOString()\n  }\n};"
      },
